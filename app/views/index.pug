extends layout

block content
  header.header
    h1=title
  main.content
    include header
    section.branches.content__branches
      h2.branches__title Branches
      ul.branches__list
        each val in branches
          li.branches__name
            a(href= val)= val
      ul.tree.branches__tree
    section.commits.content__commits
      h2.commits__title Commits
      ul.tree.commits__tree
      ul.commits__list
    section.previews.content__previews
      h2.previews__title Files
      .previews__file
      .previews__file
