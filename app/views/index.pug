extends layout

block content
  header.header
    h1=title
  main.content
    include header
    section.branches.content__branches
      h2.branches__title Branches
      ul.branches__list
        each val in branches
          li.branches__name
            a(href= val)= val
      ul.tree.branches__tree
        li.tree__folder
        li.tree__file
    section.commits.content__commits
      h2.commits__title Commits
      ul.tree.commits__tree
        li.tree__folder
        li.tree__file
      ul.commits__list
        li.commits__item
          date.commits__date
          p.commits__message
          span.commits__hash
    section.previews.content__previews
      h2.previews__title Files
      .previews__file
      .previews__file
