{
  "name": "GIT-LOC",
  "version": "1.0.0",
  "private": true,
  "description": "Homework for SHRI-2018",
  "author": "Alina Vanieva",
  "scripts": {
    "start": "gulp dev && node ./app/bin/www",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
